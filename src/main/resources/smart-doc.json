  "errorCodeDictionaries": [
    {
      "title": "通用错误码",
      "enumClassName": "com.example.demo.enums.ErrorCodeEnum",
      "codeField": "code",
      "descField": "message"
    }
  ],
  "requestExample":true,
  "responseExample":true,
  "requestParamsTable": true,
  "responseParamsTable": true,
